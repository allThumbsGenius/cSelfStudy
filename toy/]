#include<stdio.h>
#include<stdlib.h>

typedef struct _list{
	char[13] phoneNumber;
	char *personName;
	struct _list *next;
}list;

void initNode(list *l){
	l->phoneNumber = "";
	l->personName = NULL;
	l->next = NULL;
}

void addNode(list *l, char *phoneNumber, char *personName){
	list temp = l;
	list *newNode;

	newNode = (list*)malloc(sizeof(list)*1);
	newNode->phoneNumber = phoneNumber;
	newNode->personName = personName;
	newNode->next = NULL;

	while(temp->next != NULL){
		temp = temp->next;
	}

	temp->next = newNode;
}

